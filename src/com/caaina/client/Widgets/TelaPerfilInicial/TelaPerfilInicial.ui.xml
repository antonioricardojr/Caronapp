<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel ui:field="telaPerfilInicial" styleName="stylePerfilInicial">
		<g:VerticalPanel width="580px" height="85px" ui:field="painelPerfil" spacing="2" styleName="gwt-stylePerfilInicial">
			<g:Cell verticalAlignment="ALIGN_MIDDLE">
				<g:Label ui:field="nome" text="Nome" width="38px" styleName="gwt-label"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE">
				<g:Label text="Login" ui:field="login"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE">
				<g:Label text="Email" ui:field="email"/>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 