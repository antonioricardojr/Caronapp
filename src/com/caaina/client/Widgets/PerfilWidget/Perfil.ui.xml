<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel width="1024" height="640">
		<g:VerticalPanel width="788px" ui:field="gridPerfil" spacing="10">
			<g:MenuBar ui:field="menuBar">
					<g:MenuItem ui:field="usuario" text="Usuario">
						<g:MenuBar vertical="true" ui:field="menuUsuario">
							<g:MenuItem text="Logout" ui:field="logoutItem"/>
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItemSeparator/>
					<g:MenuItem text="Minhas Caronas" ui:field="minhasCaronasItem"/>
					<g:MenuItemSeparator/>
					<g:MenuItem text="Cadastrar Nova Carona" ui:field="cadastrarNovaCaronaItem"/>
					<g:MenuItemSeparator/>
					<g:MenuItem text="Buscar Caronas" ui:field="buscarCaronasItem"/>
					<g:MenuItemSeparator/>
				</g:MenuBar>
		</g:VerticalPanel>
		<g:LayoutPanel width="250px" height="150px">
			<g:layer left="0px" width="113px" top="0px" height="84px">
				<g:Label text="Nome" ui:field="campoNome"/>
			</g:layer>
			<g:layer left="0px" width="60px" top="26px" height="18px">
				<g:Label text="Endereco" ui:field="campoEndereco"/>
			</g:layer>
			<g:layer left="0px" width="60px" top="50px" height="18px">
				<g:Label text="Email" ui:field="campoEmail"/>
			</g:layer>
		</g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 